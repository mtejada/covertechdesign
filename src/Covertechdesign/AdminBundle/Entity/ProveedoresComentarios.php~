<?php

namespace Covertechdesign\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ProveedoresComentarios
 */
class ProveedoresComentarios
{
    /**
     * @var string
     */
    private $titulo;

    /**
     * @var string
     */
    private $comentario;

    /**
     * @var integer
     */
    private $idcomentario;

    /**
     * @var \Covertechdesign\AdminBundle\Entity\Proveedores
     */
    private $idproveedor;


    /**
     * Set titulo
     *
     * @param string $titulo
     * @return ProveedoresComentarios
     */
    public function setTitulo($titulo)
    {
        $this->titulo = $titulo;

        return $this;
    }

    /**
     * Get titulo
     *
     * @return string 
     */
    public function getTitulo()
    {
        return $this->titulo;
    }

    /**
     * Set comentario
     *
     * @param string $comentario
     * @return ProveedoresComentarios
     */
    public function setComentario($comentario)
    {
        $this->comentario = $comentario;

        return $this;
    }

    /**
     * Get comentario
     *
     * @return string 
     */
    public function getComentario()
    {
        return $this->comentario;
    }

    /**
     * Get idcomentario
     *
     * @return integer 
     */
    public function getIdcomentario()
    {
        return $this->idcomentario;
    }

    /**
     * Set idproveedor
     *
     * @param \Covertechdesign\AdminBundle\Entity\Proveedores $idproveedor
     * @return ProveedoresComentarios
     */
    public function setIdproveedor(\Covertechdesign\AdminBundle\Entity\Proveedores $idproveedor = null)
    {
        $this->idproveedor = $idproveedor;

        return $this;
    }

    /**
     * Get idproveedor
     *
     * @return \Covertechdesign\AdminBundle\Entity\Proveedores 
     */
    public function getIdproveedor()
    {
        return $this->idproveedor;
    }
}
